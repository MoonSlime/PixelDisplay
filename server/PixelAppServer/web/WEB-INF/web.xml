<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>Apache-Axis</display-name>
    <resource-ref>
        <res-ref-name>jdbc/pixeldisplay</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>wellsee_action.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>wellsee_action.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateInfo</servlet-name>
        <servlet-class>wellsee_action.UpdateInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Optimize</servlet-name>
        <servlet-class>wellsee_action.Optimize</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetEyeInfo</servlet-name>
        <servlet-class>wellsee_action.GetEyeInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetLastEyeInfo</servlet-name>
        <servlet-class>wellsee_action.GetLastEyeInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteEyeInfo</servlet-name>
        <servlet-class>wellsee_action.DeleteEyeInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login_nodi</servlet-name>
        <servlet-class>nodi_action.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout_nodi</servlet-name>
        <servlet-class>nodi_action.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateInfo_nodi</servlet-name>
        <servlet-class>nodi_action.UpdateInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Optimize_nodi</servlet-name>
        <servlet-class>nodi_action.Optimize</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetEyeInfo_nodi</servlet-name>
        <servlet-class>nodi_action.GetEyeInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetLastEyeInfo_nodi</servlet-name>
        <servlet-class>nodi_action.GetLastEyeInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteEyeInfo_nodi</servlet-name>
        <servlet-class>nodi_action.DeleteEyeInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prototype_Login</servlet-name>
        <servlet-class>prototype_action.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prototype_GetEyeInfo</servlet-name>
        <servlet-class>prototype_action.GetEyeInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prototype_GetRaspberryIp</servlet-name>
        <servlet-class>prototype_action.GetRaspberryIp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prototype_SetRaspberryIp</servlet-name>
        <servlet-class>prototype_action.SetRaspberryIp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prototype_SaveIntensityProfile</servlet-name>
        <servlet-class>prototype_action.SaveIntensityProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prototype_InsertEyeInfo</servlet-name>
        <servlet-class>prototype_action.InsertEyeInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Prototype_UpdateEyeInfo</servlet-name>
        <servlet-class>prototype_action.UpdateEyeInfo</servlet-class>
    </servlet>
    <servlet>
        <display-name>Apache-Axis Servlet</display-name>
        <servlet-name>AxisServlet</servlet-name>
        <servlet-class>org.apache.axis.transport.http.AxisServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>Axis Admin Servlet</display-name>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>org.apache.axis.transport.http.AdminServlet</servlet-class>
        <load-on-startup>100</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateInfo</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Optimize</servlet-name>
        <url-pattern>/optimize</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetEyeInfo</servlet-name>
        <url-pattern>/getEyeInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetLastEyeInfo</servlet-name>
        <url-pattern>/getLastEyeInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteEyeInfo</servlet-name>
        <url-pattern>/deleteEyeInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login_nodi</servlet-name>
        <url-pattern>/login_nodi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout_nodi</servlet-name>
        <url-pattern>/logout_nodi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateInfo_nodi</servlet-name>
        <url-pattern>/update_nodi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Optimize_nodi</servlet-name>
        <url-pattern>/optimize_nodi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetEyeInfo_nodi</servlet-name>
        <url-pattern>/getEyeInfo_nodi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetLastEyeInfo_nodi</servlet-name>
        <url-pattern>/getLastEyeInfo_nodi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteEyeInfo_nodi</servlet-name>
        <url-pattern>/deleteEyeInfo_nodi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Prototype_Login</servlet-name>
        <url-pattern>/prototype_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Prototype_GetEyeInfo</servlet-name>
        <url-pattern>/prototype_getEyeInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Prototype_GetRaspberryIp</servlet-name>
        <url-pattern>/prototype_getRaspberryIp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Prototype_SetRaspberryIp</servlet-name>
        <url-pattern>/prototype_setRaspberryIp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Prototype_SaveIntensityProfile</servlet-name>
        <url-pattern>/prototype_saveIntensityProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Prototype_InsertEyeInfo</servlet-name>
        <url-pattern>/prototype_insertEyeInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Prototype_UpdateEyeInfo</servlet-name>
        <url-pattern>/prototype_updateEyeInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AxisServlet</servlet-name>
        <url-pattern>/servlet/AxisServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AxisServlet</servlet-name>
        <url-pattern>*.jws</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AxisServlet</servlet-name>
        <url-pattern>/services/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/servlet/AdminServlet</url-pattern>
    </servlet-mapping>
    <mime-mapping>
        <extension>wsdl</extension>
        <mime-type>text/xml</mime-type>
    </mime-mapping>
</web-app>